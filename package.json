  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
